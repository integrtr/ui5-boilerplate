{
  "_version": "1.34.0",
  "sap.cloud": {
    "public": true,
    "service": "cloud.boilerplate"
  },
  "sap.app": {
    "id": "simple.ui.boilerplate",
    "title": "{{APPLICATION_TITLE}}",
    "type": "application",
    "i18n": "/i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "ach",
    "dataSources": {},
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "boilerplate",
          "action": "display"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "simple.ui.boilerplate.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.72.0",
      "libs": {
        "sap.m": {}
      },
      "components": []
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "simple.ui.boilerplate.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "path": "simple.ui.boilerplate.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "home",
          "pattern": "",
          "target": "home"
        }
      ],
      "targets": {
        "home": {
          "type": "View",
          "name": "Home"
        }
      }
    }
  }
}
